{
  "name": "fastreader",
  "version": "0.1.0",
  "private": true,
  "description": "A tool that enables humans to achieve great reading speeds",
  "dependencies": {
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-scripts": "1.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "extension": "bash make_extension.sh compose",
    "clean-extension": "bash make_extension.sh clean",
    "all": "yarn build && yarn extension && yarn package",
    "package": "bash make_extension.sh package",
    "clean": "rm -r ./build"
  }
}